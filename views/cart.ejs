<body>

  <div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-white border-bottom shadow-sm">
    <h5 class="my-0 mr-md-auto font-weight-normal">Catering Management</h5>
    <a href="/">back</a>
    
    
  </div>
  

    

        <div class="col-md-4 order-md-2 mb-4">
            <h4 class="d-flex justify-content-between align-items-center mb-3">
              <span class="text-muted">Your cart</span>
            </h4>

            <% order.forEach(function(order){ %>

              <div class="table-responsive">
                <table class="table table-striped table-sm">
                  <tbody>
                    <tr>
                      <td > Food Name :  <%= order.ordername %> <span class="right floated"> Price : <%=order.orderprice %> INR </span> 
                        <form action="/<%=order._id %>?_method=DELETE" method="POST">
                            <button><i class="fas fa-trash"></i></button>
                       </form></td>
                    </tr>
                    
                  </tbody>
                </table>
              </div>
              
             

           <% }) %>
          

          
           <a href="/out" class="btn btnlarge">Checkout</a>

            
   




